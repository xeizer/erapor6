{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "doctrine/dbal": "^3.0",
        "gecche/laravel-multidomain": "10.*",
        "santigarcor/laratrust": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.9",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "lavary/laravel-menu": "^1.8",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "pharaonic/livewire-select2": "^1.1",
        "rap2hpoutre/fast-excel": "^5.2",
        "riskihajar/terbilang": "^1.2",
        "larswiegers/laravel-maps": "^0.15.0",
        "staudenmeir/eloquent-eager-limit": "^1.6",
        "staudenmeir/eloquent-has-many-deep": "^1.14"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
        
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/erapor.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire-assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
